<script>

var name = prompt("What is your name?");
console.log(name);

var school = 'University of Texas';
console.log(school.length);
console.log(school.toLowerCase());

function hello(yourName) {
  return "Hello, " + yourName;
}
var name1 = hello("Aaron");
console.log(name1);
var name2 = hello("Michael");
console.log(name2);
var name3 = hello("George");
console.log(name3);

var name = prompt("What is your name?");
console.log(hello(name));

</script>

